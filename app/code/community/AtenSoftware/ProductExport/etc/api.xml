<config>
	<api>
		<resources>

			<atensoftware_productexport translate="title" module="productexport">
				<title>Aten Software Product Export API</title>
				<model>productexport/export_api</model>
				<acl>atensoftware</acl>
				<methods>
					<get_version translate="title" module="productexport">
						<title>Get Version</title>
						<acl>atensoftware</acl>
					</get_version>
					<get_export translate="title" module="productexport">
						<title>Get Product Export</title>
						<acl>atensoftware/get_export</acl>
					</get_export>
				</methods>
			</atensoftware_productexport>

		</resources>

		<acl>
			<resources>
				<atensoftware translate="title" module="productexport">
					 <title>Aten Software</title>
					 <get_export translate="title" module="productexport">
						<title>Get Product Export</title>
					 </get_export>
				</atensoftware>
				<all>
				</all>
			</resources>
		</acl>		
		
	</api>
</config>